{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun --hot src/server/index.tsx\" \"bunx tsc --noEmit --watch\"",
    "build": "bun build src/client/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/server/index.tsx",
    "format": "biome format --write src"
  },
  "dependencies": {
    "@kkestell/typst": "0.13.1",
    "jszip": "^3.10.1",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "biome": "^0.3.3",
    "concurrently": "^9.2.1"
  }
}
